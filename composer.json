{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/phobiavr/phober-laravel-common"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Abdukhaligov/nova.git"
        },
        {
            "type": "path",
            "url": "./nova-components/Custom"
        }
    ],
    "require": {
        "php": "^8.2",
        "datomatic/nova-enum-field": "^1.9",
        "ebess/advanced-nova-media-library": "^4.4",
        "kirschbaum-development/nova-comments": "^4.0",
        "laravel/framework": "^11.9",
        "laravel/nova": "^4.35",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.29",
        "lednerb/nova-action-button-selectors": "^2.0",
        "phobiavr/phober-laravel-common": "^0.1.0",
        "spatie/laravel-medialibrary": "^11.12",
        "spatie/laravel-translatable": "^6.10",
        "spatie/nova-translatable": "^4.1",
        "staudenmeir/eloquent-has-many-deep": "^1.20",
        "yajra/laravel-oci8": "^11.6",
        "nova/custom": "@dev"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
